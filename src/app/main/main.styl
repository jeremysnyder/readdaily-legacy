md-card-header-pre
  display inline-block
  width 50px
  text-align center
  background-color #DDD
  border-top-left-radius 2px
  border-bottom-left-radius 2px
  color #777

  h3, h4
    margin 5px

.md-title
  padding-left 5px

md-fab-trigger
  button.md-fab.md-button:not([disabled])
    background-color #656

    &:hover
      background-color #444

md-fab-actions
  button
    span
      color #111
